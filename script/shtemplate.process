# Declare App Name
APP_NAME=`basename ${0}`
APP_NAME="${APP_NAME%.*}"

# Load configuration
. ${HOME}/.apprc										# .mainrc@ -> ${HOME}/shtemplate/config/main.properties
. ${HOME}/.javarc										# .javarc@ -> ${HOME}/shtemplate/config/java.properties
. ${HOME}/.${APP_NAME}rc						# .batchnoerc@ -> ${HOME}/batch/BatchNOE/cfg/batchnoe.properties

# Load libraries
. ${APP_JVNDR}/tCommons.lib					# ${HOME}/shtemplate/vendor/tCommons.lib
. ${APP_JVNDR}/jCommons.lib					# ${HOME}/shtemplate/vendor/jCommons.lib

# Load App Process
. ${APP_JSCRT}/rmi.process
. ${APP_JSCRT}/server.process
. ${APP_JSCRT}/class.process

DebugOff
jLoadProperties
jShowProperties

if [ -L "${APP_JABIN}/rmi.start" ]
then
  rmiVersion
fi

if [ -L "${APP_JABIN}/server.start" ]
then
  serverVersion
fi

if [ -L "${APP_JABIN}/class.start" ]
then
  classVersion
fi




