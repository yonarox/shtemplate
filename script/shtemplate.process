# Declare App Name
APP_NAME=`basename ${0}`
APP_NAME="${APP_NAME%.*}"

# Load configuration
. ${HOME}/.javarc
. ${HOME}/.${APP_NAME}rc

# Load Libraries
. ${APP_JVNDR}/tCommons.lib
. ${APP_JVNDR}/jCommons.lib

# Load App Process
. ${APP_JSCRT}/rmi.process
. ${APP_JSCRT}/server.process
. ${APP_JSCRT}/class.process

DebugOff
jLoadProperties
jShowProperties

if [ -L "${APP_JABIN}/rmi.start" ]
then
  rmiVersion
fi

if [ -L "${APP_JABIN}/server.start" ]
then
  serverVersion
fi

if [ -L "${APP_JABIN}/class.start" ]
then
  classVersion
fi




