# Load an RMI Process

# Load main configuration
. ${HOME}/.javarc

# Load common functions
. ${APP_JVNDR}/tCommons.lib
. ${APP_JVNDR}/jCommons.lib

# Load properties of application
jLoadProperties "${0%.st*}"

# Load JAVA Environment
JavaEnvironment "java16"
PrintOut "JVM: ${JAVA_VERSION}"

# Prepare to execute java app
cd ${APP_JHOME}
jLoadRMIProcess "BatchNOE.jar" "10010"

